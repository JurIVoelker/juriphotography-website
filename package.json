{
  "name": "juriphotography-homepage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "axios": "^1.7.2",
    "next": "14.2.4",
    "react": "^18",
    "react-aria": "^3.33.1",
    "react-aria-components": "^1.2.1",
    "react-dom": "^18",
    "react-loading-skeleton": "^3.4.0",
    "resend": "^3.4.0",
    "use-debounce": "^10.0.1"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  },
  "scripts": {
    "dev": "concurrently -p \"[{name}]\" -n \"_backend,frontend\" -c \"blue.bold,green.bold\" \"bun run dev:backend\" \"bun run dev:frontend\"",
    "dev:frontend": "wait-on http://127.0.0.1:1337 && cd frontend && bun run dev",
    "dev:backend": "cd backend && bun run develop"
  }
}
